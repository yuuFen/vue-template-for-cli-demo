import Vue from 'vue'
import VueRouter from 'vue-router'
import Home from './views/Home.vue'

Vue.use(VueRouter)

  const routes = [
  {
    path: '/',
    name: 'Home',
    component: Home
  },
  {{#each list}}
  {
    path: '/{{name}}',
    name: '{{name}}',
    component: () => import('./views/{{file}}')
  },
  {{/each}}
]

const router = new VueRouter({
  routes
})

export default router
